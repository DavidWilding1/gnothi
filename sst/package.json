{
  "name": "gnothi",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "AWS_PROFILE=gnothiadmin sst start --outputs-file \".env.json\"",
    "build": "AWS_PROFILE=gnothiadmin sst build",
    "deploy": "AWS_PROFILE=gnothiadmin sst deploy --stage=production",
    "remove": "AWS_PROFILE=gnothiadmin sst remove",
    "console": "AWS_PROFILE=gnothiadmin sst console",
    "typecheck": "AWS_PROFILE=gnothiadmin tsc --noEmit",
    "test": "AWS_PROFILE=gnothiadmin node backend/test/convertEnv.js && vitest run"
  },
  "devDependencies": {
    "@serverless-stack/cli": "^1.2.19",
    "@serverless-stack/resources": "^1.2.19",
    "@tsconfig/node16": "^1.0.2",
    "@types/lodash": "^4.14.182",
    "aws-cdk-lib": "2.24.0",
    "typescript": "^4.7.3",
    "vitest": "^0.14.2"
  },
  "workspaces": [
    "backend"
  ],
  "dependencies": {
    "@aws-sdk/client-rds-data": "^3.105.0",
    "zod": "^3.17.3"
  }
}
