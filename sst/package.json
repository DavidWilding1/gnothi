{
  "name": "sst",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "test": "AWS_PROFILE=gnothiadmin sst test",
    "test-watch": "AWS_PROFILE=gnothiadmin node test/convertEnv.js && sst test --watch --silent=false",
    "start": "AWS_PROFILE=gnothiadmin sst start --outputs-file \".env.js\"",
    "build": "AWS_PROFILE=gnothiadmin sst build",
    "deploy": "AWS_PROFILE=gnothiadmin sst deploy",
    "remove": "AWS_PROFILE=gnothiadmin sst remove"
  },
  "exports": {
    "src": "./src"
  },
  "eslintConfig": {
    "extends": [
      "serverless-stack"
    ]
  },
  "devDependencies": {
    "@serverless-stack/cli": "0.69.3",
    "@serverless-stack/resources": "0.69.3",
    "@tsconfig/node14": "^1.0.1",
    "@types/aws-lambda": "^8.10.70",
    "@types/lodash": "^4.14.181",
    "@types/node": "<15.0.0",
    "aws-cdk-lib": "^2.19.0",
    "kysely": "^0.17.1",
    "typescript": "4.4.4"
  },
  "dependencies": {
    "@aws-sdk/client-rds-data": "^3.58.0",
    "kysely-data-api": "^0.0.9",
    "zod": "^3.14.3"
  },
  "jest": {
    "testMatch": [
      "<rootDir>/src/**/*.test.ts"
    ]
  }
}
